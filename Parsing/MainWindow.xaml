        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Parsing"
        mc:Ignorable="d"
        Title="MainWindow" Height="295" Width="800" MinWidth="800" MinHeight="295" MaxHeight="295" >
