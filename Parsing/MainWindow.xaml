        <ComboBox x:Name="comboBox1" HorizontalAlignment="Left" Margin="145,93,0,0" VerticalAlignment="Top" Width="190" />
        <ComboBox x:Name="comboBox2" HorizontalAlignment="Right" Margin="0,93,10,0" VerticalAlignment="Top" Width="190" />
